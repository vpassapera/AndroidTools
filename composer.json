{
    "name": "tdn/android-tools",
    "description": "A collection of commands to do things with Android/Android apps, etc.",
    "type": "application",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Passapera",
            "email": "vpassapera@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/console": "~2.7",
        "symfony/filesystem": "~2.7",
        "symfony/finder": "~2.7",
        "doctrine/collections": "~1.3",
        "tdn/php-types": "*@stable"
    },
    "require-dev": {
        "kherge/box": "~2.5",
        "fabpot/php-cs-fixer": "~1",
        "phpunit/phpunit": "~4.7",
        "squizlabs/php_codesniffer": "~2.3",
        "apigen/apigen": "dev-master",
        "mockery/mockery": "~0.9",
        "raulfraile/ladybug": "~1.0",
        "satooshi/php-coveralls": "~0.6",
        "codegyre/robo": "~0.5",
        "jakub-onderka/php-parallel-lint": "0.*",
        "jakub-onderka/php-console-highlighter": "0.*"
    },
    "autoload": {
        "psr-4": {
            "Tdn\\AndroidTools\\": ""
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "prefer-stable": true,
    "bin": ["android-tools"]
}
